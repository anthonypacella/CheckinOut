 <div class="row">
    {{#each books as |book|}}
        <div class="col-md-4"> 
            <div class="card my-2">
                <div class="card-header">
                    <h3>{{book.title}}</h3>
                </div>
                <div class="card-body">
                    <div class="card-title">
                        <h5>{{book.author}}</h5>
                    </div>
                    <div class="card-text">
                        <p>{{book.subject}}</p>
                    </div>
                </div>   
            </div> 
        </div>
    {{/each}}
</div>

